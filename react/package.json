  "name": "simple-photo-management-react-frontend",
  "version": "1.1.0-beta",
  "private": true,
  "description": "React frontend for Django REST Simple Photo Management application",
  "main": "index.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "*",
    "@fortawesome/free-brands-svg-icons": "*",
    "@fortawesome/free-regular-svg-icons": "*",
    "@fortawesome/free-solid-svg-icons": "*",
    "@fortawesome/react-fontawesome": "*",
    "@popperjs/core": "^2.9.3",
    "acorn": "*",
    "axios": "*",
    "bootstrap": "*",
    "env-cmd": "*",
    "jquery": "*",
    "js-cookie": "*",
    "lodash.clonedeep": "*",
    "moment": "*",
    "moment-timezone": "*",
    "popper.js": "*",
    "react": "*",
    "react-app-polyfill": "*",
    "react-dom": "*",
    "react-markdown": "*",
    "react-modal": "*",
    "react-modal-image": "*",
    "react-router-dom": "*",
    "react-scripts": "*",
    "to": "*"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "PORT=3001 HTTPS=false BROWSER=none react-scripts start",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "test:once": "CI=true react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "eject": "react-scripts eject",
    "preinstall": "npx npm-force-resolutions"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-plugin-react": "^7.24.0"
  },
  "resolutions": {
    "set-value": "^4.0.1",
    "ansi-html": "*",
    "ansi-regex": "*",
    "nth-check": "*",
    "glob-parent": "*",
    "immer": "*",
    "browserlist": "*",
    "optimist": "*",
    "minimist": "*",
    "react-dev-utils":"*"
  }
}
